<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>r1155.ac</path>

  <animation>
    <object-name>r1155</object-name>
    <object-name>zone</object-name>
    <object-name>poignees</object-name>
    <object-name>visses</object-name>
    <object-name>needle</object-name>
    <object-name>marques</object-name>
    <object-name>cadran</object-name>

    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>noshadow</type>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>glass.png</texture>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>material</type>
    <object-name>zone</object-name>
    <emission>
      <red>   1.0 </red>
      <green> 0.2 </green>
      <blue>  0.0 </blue>
      <factor-prop>/systems/electrical/outputs/instrument-lights</factor-prop>
    </emission>
  </animation>

